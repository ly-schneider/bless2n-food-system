# Copy this file to .env and fill in your values

# -- DATABASE --
POSTGRES_USER=rentro_user
POSTGRES_PASSWORD=rentro_password
POSTGRES_DB=rentro
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# -- REDIS --
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=redis_password
REDIS_DB=0

# -- ASYNQMON --
ASYNQMON_PORT=8081

# -- APPLICATION --
APP_ENV=local
APP_PORT=8080
READ_TIMEOUT=10s
WRITE_TIMEOUT=15s

# -- LOGGER --
LOG_LEVEL=info
LOG_DEVELOPMENT=true

# -- DOCKER COMPOSE --
# Override for containerized services
POSTGRES_HOST_DOCKER=postgres
REDIS_HOST_DOCKER=redis
REDIS_PORT_DOCKER=6379

{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "docker-compose-up-build",
      "type": "shell",
      "command": "docker compose -f ${workspaceFolder}/docker-compose.yml --env-file ${workspaceFolder}/.env.development up --build",
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": ".",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".",
          "endsPattern": "."
        }
      }
    }
  ]
}
